{
    "name": "superbolt/superbolt-symfony",
    "type": "symfony-bundle",
    "description": "Symfony adapter for Superbolt",
    "keywords": ["superbolt", "Symfony"],
    "homepage": "https://github.com/superbolt/superbolt-symfony",
    "license": "",
    "authors": [
        {
            "name": "Superbolt",
            "email": "info@superbolt.app",
            "homepage": "https://github.com/superboltapp"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.5",
        "symfony/config": "^4.0||^5.0",
        "symfony/console": "^4.0||^5.0",
        "symfony/dependency-injection": "^4.0||^5.0",
        "symfony/event-dispatcher": "^4.0||^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "symplify/easy-coding-standard": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Superbolt\\SuperboltBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superbolt\\SuperboltBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
